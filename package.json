    "core-js": "^3.32.2",
    "earcut": "^2.2.4",
    "font-awesome": "4.7.0",
    "immer": "^9.0.21",
    "jsketcher-occ-engine": "1.0.1-f505cdcb9f37685cdadb937bc3b11b9b75f9267c",
    "jszip": "^3.10.1",
    "libtess": "1.2.2",
    "lodash": "^4.17.21",
    "marked": "^4.3.0",
    "mousetrap": "^1.6.5",
    "numeric": "1.2.6",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
